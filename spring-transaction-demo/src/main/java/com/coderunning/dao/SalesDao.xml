<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.coderunning.dao.SalesDao">

    <insert id="insertSalesGoods" >
        insert into sales (id, gid, nums )
        values (#{id}, #{gid}, #{nums});

    </insert>

</mapper>